<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="325.0" prefWidth="453.0" xmlns="http://javafx.com/javafx/9.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.simplyrugby.controllers.NotesController">
    <children>
        <ListView fx:id="lstNotes" prefHeight="290.0" prefWidth="453.0"/>
        <Button fx:id="btnDeleteSelected" layoutX="340.0" layoutY="294.0" mnemonicParsing="false"
                onAction="#btnDeleteSelectedClickHandler" text="Delete Selected"/>
        <Button fx:id="btnAddNotes" layoutX="255.0" layoutY="294.0" mnemonicParsing="false"
                onAction="#btnAddNotesClickHandler" text="Add Notes"/>
    </children>
</AnchorPane>
